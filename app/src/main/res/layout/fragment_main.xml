<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/primary_500">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/text_view_phone_number"
        android:layout_width="0dp"
        android:layout_height="@dimen/text_view_title_height_max"
        android:background="@drawable/ic_round_card_view"
        android:hint="@string/text_view_phone_number"
        android:padding="@dimen/text_view_padding_max"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textColorHint="@color/primary_200"
        android:textIsSelectable="true"
        android:textSize="@dimen/text_view_size_big"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_view_phone_number">

        <com.google.android.material.button.MaterialButton
            android:id="@+id/button_1"
            style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
            android:layout_width="@dimen/material_button_size"
            android:layout_height="@dimen/material_button_size"
            android:layout_marginTop="@dimen/layout_margin_med"
            android:layout_marginBottom="@dimen/layout_margin_med"
            android:backgroundTint="@color/primary_400"
            android:text="1"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text_view_size_max"
            app:cornerRadius="@dimen/button_radius_max"
            app:layout_constraintBottom_toBottomOf="@+id/button_2"
            app:layout_constraintEnd_toStartOf="@+id/button_2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/button_2"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/button_2"
            style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
            android:layout_width="@dimen/material_button_size"
            android:layout_height="@dimen/material_button_size"
            android:layout_marginTop="@dimen/layout_margin_med"
            android:layout_marginBottom="@dimen/layout_margin_med"
            android:backgroundTint="@color/primary_400"
            android:text="2"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text_view_size_max"
            app:cornerRadius="@dimen/button_radius_max"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/button_3"
            style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
            android:layout_width="@dimen/material_button_size"
            android:layout_height="@dimen/material_button_size"
            android:layout_marginTop="@dimen/layout_margin_med"
            android:layout_marginBottom="@dimen/layout_margin_med"
            android:backgroundTint="@color/primary_400"
            android:text="3"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text_view_size_max"
            app:cornerRadius="@dimen/button_radius_max"
            app:layout_constraintBottom_toBottomOf="@+id/button_2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/button_2"
            app:layout_constraintTop_toTopOf="@+id/button_2"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/button_4"
            style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
            android:layout_width="@dimen/material_button_size"
            android:layout_height="@dimen/material_button_size"
            android:layout_marginTop="@dimen/layout_margin_med"
            android:layout_marginBottom="@dimen/layout_margin_med"
            android:backgroundTint="@color/primary_400"
            android:text="4"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text_view_size_max"
            app:cornerRadius="@dimen/button_radius_max"
            app:layout_constraintBottom_toBottomOf="@+id/button_5"
            app:layout_constraintEnd_toStartOf="@+id/button_5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/button_5"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/button_5"
            style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
            android:layout_width="@dimen/material_button_size"
            android:layout_height="@dimen/material_button_size"
            android:layout_marginTop="@dimen/layout_margin_med"
            android:layout_marginBottom="@dimen/layout_margin_med"
            android:backgroundTint="@color/primary_400"
            android:text="5"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text_view_size_max"
            app:cornerRadius="@dimen/button_radius_max"
            app:layout_constraintEnd_toEndOf="@+id/button_2"
            app:layout_constraintStart_toStartOf="@+id/button_2"
            app:layout_constraintTop_toBottomOf="@+id/button_2"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/button_6"
            style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
            android:layout_width="@dimen/material_button_size"
            android:layout_height="@dimen/material_button_size"
            android:layout_marginTop="@dimen/layout_margin_med"
            android:layout_marginBottom="@dimen/layout_margin_med"
            android:backgroundTint="@color/primary_400"
            android:text="6"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text_view_size_max"
            app:cornerRadius="@dimen/button_radius_max"
            app:layout_constraintBottom_toBottomOf="@+id/button_5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/button_5"
            app:layout_constraintTop_toTopOf="@+id/button_5"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/button_7"
            style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
            android:layout_width="@dimen/material_button_size"
            android:layout_height="@dimen/material_button_size"
            android:layout_marginTop="@dimen/layout_margin_med"
            android:layout_marginBottom="@dimen/layout_margin_med"
            android:backgroundTint="@color/primary_400"
            android:text="7"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text_view_size_max"
            app:cornerRadius="@dimen/button_radius_max"
            app:layout_constraintBottom_toBottomOf="@+id/button_8"
            app:layout_constraintEnd_toStartOf="@+id/button_8"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/button_8"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/button_8"

            style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
            android:layout_width="@dimen/material_button_size"
            android:layout_height="@dimen/material_button_size"
            android:layout_marginTop="@dimen/layout_margin_med"
            android:layout_marginBottom="@dimen/layout_margin_med"
            android:backgroundTint="@color/primary_400"
            android:text="8"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text_view_size_max"
            app:cornerRadius="@dimen/button_radius_max"
            app:layout_constraintEnd_toEndOf="@+id/button_5"
            app:layout_constraintStart_toStartOf="@+id/button_5"
            app:layout_constraintTop_toBottomOf="@+id/button_5"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/button_9"
            style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
            android:layout_width="@dimen/material_button_size"
            android:layout_height="@dimen/material_button_size"
            android:layout_marginTop="@dimen/layout_margin_med"
            android:layout_marginBottom="@dimen/layout_margin_med"
            android:backgroundTint="@color/primary_400"
            android:text="9"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text_view_size_max"
            app:cornerRadius="@dimen/button_radius_max"
            app:layout_constraintBottom_toBottomOf="@+id/button_8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/button_8"
            app:layout_constraintTop_toTopOf="@+id/button_8"
            tools:ignore="HardcodedText" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/button_0"
            style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
            android:layout_width="@dimen/material_button_size"
            android:layout_height="@dimen/material_button_size"
            android:layout_marginTop="@dimen/layout_margin_med"
            android:layout_marginBottom="@dimen/layout_margin_med"
            android:backgroundTint="@color/primary_400"
            android:text="0"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text_view_size_max"
            app:cornerRadius="@dimen/button_radius_max"
            app:layout_constraintEnd_toEndOf="@+id/button_8"
            app:layout_constraintStart_toStartOf="@+id/button_8"
            app:layout_constraintTop_toBottomOf="@+id/button_8"
            tools:ignore="HardcodedText" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/floating_button_open_whatsapp"
            android:layout_width="@dimen/material_button_size"
            android:layout_height="@dimen/material_button_size"
            android:layout_marginTop="@dimen/layout_margin_med"
            android:layout_marginBottom="@dimen/layout_margin_med"
            android:contentDescription="@string/floating_button_whatsapp"
            android:src="@drawable/ic_round_whatsapp"
            app:fabCustomSize="@dimen/material_button_size"
            app:layout_constraintBottom_toBottomOf="@+id/button_0"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/button_0"
            app:layout_constraintTop_toTopOf="@+id/button_0"
            app:maxImageSize="@dimen/image_view_icon_size"
            app:tint="@color/primary_500" />

        <com.google.android.material.button.MaterialButton
            android:id="@+id/button_backspace"
            style="@style/Widget.MaterialComponents.ExtendedFloatingActionButton"
            android:layout_width="@dimen/material_button_size"
            android:layout_height="@dimen/material_button_size"
            android:layout_marginTop="@dimen/layout_margin_med"
            android:layout_marginBottom="@dimen/layout_margin_med"
            android:backgroundTint="@color/primary_400"
            android:text="\u232b"
            android:textColor="@android:color/white"
            android:textSize="@dimen/text_view_size_max"
            app:cornerRadius="@dimen/button_radius_max"
            app:layout_constraintBottom_toBottomOf="@+id/button_0"
            app:layout_constraintEnd_toStartOf="@+id/button_0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/button_0"
            tools:ignore="HardcodedText" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>